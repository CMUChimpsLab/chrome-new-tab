{
  "name": "SocialSafety",
  "description": "SocialSafety helps you fight evil things on the internet.",
  "version": "0.9",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 9,
  "web_accessible_resources": ["blank_ntp_est.html"],
  "content_security_policy":
    "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js; object-src 'self'",
  "permissions": ["http://localhost/*", "storage", "activeTab", "webRequest", "webRequestBlocking", "*://*.facebook.com/"],
  "optional_permissions": ["cookies", "*://*.facebook.com/"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "icons": {
    "16": "images/icons/16/icon2-16.png",
    "32": "images/icons/32/icon2-32.png",
    "48": "images/icons/48/icon2-48.png",
    "128": "images/icons/128/icon2-128.png"
  }
}
